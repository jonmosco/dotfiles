[alias]
  c = commit
  comit = commit
  co = checkout
  lg = "log --pretty=format:'%C(yellow)%h%C(reset) %C(blue)%an%C(reset) %C(cyan)%cr%C(reset) %s %C(green)%d%C(reset)' --graph --date-order"
  p = push
  rb = rebase
  s  = status
  st = status --short --branch
  fup = fetch upstream

[core]
    excludesfile = ~/.gitignore_global

[color]
  ui = auto

[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
  editor = vim

[push]
  default = current
